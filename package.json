{
    "name": "ping",
    "description": "A minimal chat application.",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "build": "turbo build",
        "dev": "turbo dev",
        "android": "turbo android --filter=mobile",
        "ios": "turbo ios --filter=mobile",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "@turbo/gen": "^1.9.7",
        "eslint": "^7.32.0",
        "prettier": "^2.5.1",
        "turbo": "latest"
    },
    "packageManager": "yarn@1.22.19",
    "workspaces": [
        "apps/*",
        "packages/*"
    ]
}